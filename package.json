{
  "name": "selfbrowser",
  "version": "1.0.0",
  "description": "A modern web browser built using the MERN stack",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "build": "npm run client:build && npm run server:build",
    "test": "npm run client:test && npm run server:test",
    "lint": "npm run client:lint && npm run server:lint",
    "format": "prettier --write .",
    "client:dev": "cd client && npm run dev",
    "client:build": "cd client && npm run build",
    "client:test": "cd client && npm run test",
    "client:lint": "cd client && npm run lint",
    "server:dev": "cd server && npm run dev",
    "server:build": "cd server && npm run build",
    "server:test": "cd server && npm run test",
    "server:lint": "cd server && npm run lint",
    "electron:dev": "electron .",
    "electron:build": "electron-builder"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4",
    "prettier": "^3.0.3"
  },
  "author": "Your Name",
  "license": "MIT"
}